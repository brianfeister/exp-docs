{
  "gitbook": "3.2.2",
  "title": "EXP Docs",
  "plugins": ["edit-link", "prism", "-highlight", "github"],
  "pluginsConfig": {
    "fontsettings": {
      "theme": "night",
      "family": "sans",
      "size": 2
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "edit-link": {
      "base": "https://github.com/brianfeister/exp-docs/tree/master",
      "label": "Edit This Page"
    },
    "github": {
      "url": "https://github.com/brianfeister/exp-docs/"
    }
  },
  "styles": {
    "website": "styles/website.css"
  }
}
